            gtag('js', new Date());
            window.addEventListener('load', function () {
                setTimeout(function () {
                    var loadTime = window.performance.timing.loadEventEnd - window.performance.timing.navigationStart;
                    //console.log("loadTime : " + loadTime);
                    var url = window.location.href;
                    loadTime = Math.round(loadTime / 100) / 10;
                    const d = new Date();
                    var dateTime = d.getFullYear().toString() + d.getMonth().toString() + d.getDate().toString() + d.getHours().toString() + d.getMinutes().toString() + d.getSeconds().toString() + d.getMilliseconds().toString();
                    var uniqueId = Math.random().toString(36).substr(2, 9);
                    gtag('event', 'Page_Load_Speed', {
                        'event_category': url,
                        'event_label': uniqueId+"_"+dateTime,
                        'value': loadTime,
                        'send_to': 'G-SJDBYDPX4S'
                    });
                }, 0);
            });

            gtag('config', 'G-SJDBYDPX4S');
